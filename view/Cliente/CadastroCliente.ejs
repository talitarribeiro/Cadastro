<html>
    <body>
        <h1> Cadastro de Cliente</h1>
        <form action="/cadastro/cliente/salvar" method="POST">
            <label>Nome:</label>
            <input name="Nome" type="text"/><br/>
            <label>Senha:</label>
            <input name="senha" type="password"/><br/>
            <input type="submit" value="cadastro"/>
        </form>
        <% if(mensagem) { %>
            <p>
                <label style= "color:rgb(7, 6, 6)"><%mensagem %></label>
            </p>

        <%}%>

    </body>
</html>